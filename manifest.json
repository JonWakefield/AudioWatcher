{
  "name": "AudioWatcher",
  "version": "0.0.2",
  "description": "AudioWatcher mutes & unmutes audio from a set tab when other browser-based audio is detected",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AudioGUy"
  },
  "manifest_version": 3
}
